<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html b:css='false' b:defaultwidgetversion='2' b:layoutsVersion='3' b:responsive='true' b:templateUrl='indie.xml' b:templateVersion='1.3.0' expr:dir='data:blog.languageDirection' expr:lang='data:blog.locale.language' xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>
  {{!-- <head> Open --}}
  &lt;head&gt;
    {{!-- Meta tags --}}
    <meta expr:charset='data:blog.encoding'/>
    <meta content='width=device-width, initial-scale=1, user-scalable=1, minimum-scale=1, maximum-scale=5' name='viewport'/>

    {{!-- General tags --}}
    <title><data:view.title.escaped/></title>

    {{> partial-meta-header}}

    {{> skin}}
    {{!-- Default markups --}}
    <b:defaultmarkups>
      {{!-- Clean all inclusions of all widgets automatically  --}}
      {{#each defaultmarkups}}
      <b:defaultmarkup type='{{@key}}'>
        {{#each this}}
        <b:includable id='{{this}}'/>
        {{/each}}
      </b:defaultmarkup>
      {{/each}}
    </b:defaultmarkups>

  {{!-- </head> Close --}}
  &lt;!--<head/>--&gt;&lt;/head&gt;

  {{!-- <body> open ] --}}
  <body class='' id='mainContent'>
    <b:if cond='data:view.isLayoutMode'>
      <b:section id='main'/>
    </b:if>

    <b:tag class='mainWrapper' cond='!data:view.isError' name='div'>
      <b:if cond='!data:view.isError'>
        {{!-- [ Header section ] --}}
        <header class='header' id='header'>
          {{> partial-header}}
        </header>
      </b:if>
    </b:tag>

  {{!-- </body> close ] --}}
  &lt;!--</body>--&gt;&lt;/body&gt;
</html>